{
  "name": "puppet-jenkins",
  "version": "0.0.5",
  "author": "Joseph Karns",
  "license": "Apache-2.0",
  "summary": "Installs, configures, and manages the Jenkins2 service.",
  "source": "https://github.com/jmk61/puppet-jenkins.git",
  "project_page": "https://github.com/jmk61/puppet-jenkins",
  "issues_url": "https://github.com/jmk61/puppet-jenkins/issues",
  "dependencies": [
  	{
  		"name":"puppetlabs/stdlib",
  		"version_requirement":">= 3.2.0 < 5.0.0"
  	},
  	{
  		"name":"puppetlabs/apt",
  		"version_requirement": ">= 2.0.1 < 3.0.0"
  	},
  	{ 
  		"name": "puppetlabs/java",
  		"version_requirement": ">= 1.1.1 < 2.0.0"
  	},
  	{
		"name": "puppet/archive",
		"version_requirement": ">= 0.4.8 < 2.0.0"
	}
  ],
  "data_provider": "hiera",
  "tags": [ "jenkins" ],  
  "operatingsystem_support": [
  	{
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
      	"14.04",
      	"12.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.5.0 < 5.0.0"
    },
    {
      "name": "pe",
      "version_requirement": ">= 2015.2.0 < 2016.4.0"
    }
  ]
}